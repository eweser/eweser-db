{
  "name": "@eweser/db",
  "version": "1.13.2",
  "description": "eweser-db core database",
  "keywords": [
    "user-owned",
    "crdt",
    "interoperable",
    "local-first"
  ],
  "author": "jacobcoro",
  "type": "module",
  "homepage": "https://github.com/eweser/eweserdb",
  "license": "MIT",
  "directories": {
    "src": "src"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eweser/eweserdb.git"
  },
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "main": "./dist/db.umd.cjs",
  "module": "./dist/db.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/db.js",
      "require": "./dist/db.umd.cjs"
    }
  },
  "scripts": {
    "test": "vitest --run",
    "build": "rm -rf dist; rm -rf types; tsc --project tsconfig.build.json && npx vite build",
    "typescript-watch": "tsc --watch --project tsconfig.build.json",
    "vite-build-watch": "vite build --watch",
    "dev": "run-p vite-build-watch typescript-watch",
    "clean": "rm -rf dist; rm -rf types",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ."
  },
  "bugs": {
    "url": "https://github.com/eweser/eweserdb/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eweser/eslint-config-ts": "^1.13.2",
    "@types/request": "^2.48.12",
    "eslint": "^9.17.0",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "gitHead": "d6ee91d84092583f851548fa9ffe8ccf5de5705a",
  "dependencies": {
    "@eweser/shared": "^1.13.2",
    "@y-sweet/client": "^0.7.0",
    "y-indexeddb": "^9.0.12",
    "y-webrtc": "^10.3.0",
    "yjs": "^13.6.21",
    "yjs-types": "^0.0.1"
  }
}
