{
  "name": "@eweser/db",
  "version": "1.0.1",
  "description": "eweser-db core database",
  "keywords": [
    "user-owned",
    "crdt",
    "matrix",
    "local-first"
  ],
  "author": "jacobcoro",
  "type": "module",
  "homepage": "https://github.com/jacobcoro/eweserdb",
  "license": "MIT",
  "directories": {
    "src": "src"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobcoro/eweserdb.git"
  },
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "main": "./dist/db.umd.cjs",
  "module": "./dist/db.js",
  "exports": {
    ".": {
      "import": "./dist/db.js",
      "require": "./dist/db.umd.cjs"
    }
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsc && vite build",
    "dev": "tsc --watch && vite build --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ."
  },
  "bugs": {
    "url": "https://github.com/jacobcoro/eweserdb/issues"
  },
  "peerDependencies": {
    "matrix-crdt": "0.2.1-alpha.1",
    "matrix-js-sdk": "^24.0.0",
    "y-indexeddb": "^9.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eweser/eslint-config": "^1.0.1",
    "@matrix-org/olm": "^3.2.14",
    "cypress": "^10.8.0",
    "fake-indexeddb": "^4.0.1",
    "vite-plugin-dts": "^1.5.0",
    "yjs-types": "^0.0.1"
  },
  "gitHead": "3d39fd98760efd13963caa96db636df5cd985625",
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
