{
  "name": "@eweser/db",
  "version": "1.6.2",
  "description": "eweser-db core database",
  "keywords": [
    "user-owned",
    "crdt",
    "matrix",
    "local-first"
  ],
  "author": "jacobcoro",
  "type": "module",
  "homepage": "https://github.com/eweser/eweser-db",
  "license": "MIT",
  "directories": {
    "src": "src"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobcoro/eweserdb.git"
  },
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "main": "./dist/db.umd.cjs",
  "module": "./dist/db.js",
  "exports": {
    ".": {
      "import": "./dist/db.js",
      "require": "./dist/db.umd.cjs",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 vitest --run",
    "test:watch": "NODE_TLS_REJECT_UNAUTHORIZED=0 vitest --watch",
    "build": "rm -rf dist; rm -rf types; tsc --project tsconfig.build.json && vite build",
    "typescript-watch": "tsc --watch --project tsconfig.build.json",
    "vite-build-watch": "vite build --watch",
    "dev": "run-p vite-build-watch typescript-watch",
    "clean": "rm -rf dist; rm -rf types",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ."
  },
  "bugs": {
    "url": "https://github.com/jacobcoro/eweserdb/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eweser/eslint-config-ts": "^1.4.0",
    "@types/request": "^2.48.8",
    "eslint": "^8.37.0",
    "fake-indexeddb": "^4.0.1",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "typescript": "^4.4.4",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  },
  "gitHead": "da7009f0729d7f3e60b30d7d26c85bc67ecb17a5",
  "dependencies": {
    "@matrix-org/olm": "^3.2.14",
    "@eweser/websockets": "*",
    "matrix-crdt": "0.2.1-alpha.1",
    "matrix-js-sdk": "^19.4.0",
    "y-indexeddb": "^9.0.9",
    "y-protocols": "^1.0.5",
    "y-webrtc": "^10.2.5",
    "yjs": "^13.5.16",
    "yjs-types": "^0.0.1"
  }
}
