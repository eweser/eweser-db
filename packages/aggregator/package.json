{
  "name": "@eweser/aggregator",
  "version": "1.0.0",
  "description": "Listens to changes in eweser database rooms marked as public, and serves changes over an API",
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 vitest",
    "test:watch": "NODE_TLS_REJECT_UNAUTHORIZED=0 vitest --watch",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "build": "tsc",
    "start": "tsc; node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eweser/eweser-db.git"
  },
  "keywords": [
    "user-owned",
    "crdt",
    "matrix",
    "local-first"
  ],
  "author": "jacobcoro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eweser/eweser-db/issues"
  },
  "type": "module",
  "homepage": "https://github.com/eweser/eweser-db#readme",
  "dependencies": {
    "@eweser/db": "*",
    "@eweser/websockets": "*",
    "matrix-crdt": "^0.2.1-alpha.1",
    "matrix-js-sdk": "19.4.0",
    "mongodb": "^5.6.0",
    "node-fetch": "^3.3.1",
    "ws": "^8.13.0",
    "yjs": "^13.5.16"
  },
  "devDependencies": {
    "@eweser/eslint-config-ts": "^1.4.0",
    "@types/ws": "^8.5.5",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "transpileOnly": true
  }
}
